{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {},
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": 0,
    "no-eq-null": 0,
    "no-unused-expressions": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "indent": [
      2,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "no-with": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "array-bracket-spacing": [
      2,
      "always"
    ]
  }
}
